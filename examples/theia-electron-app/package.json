  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Theia",
        "electron": {
          "disallowReloadKeybinding": true,
          "windowOptions": {
            "webPreferences": {
              "nodeIntegration": true,
              "contextIsolation": false,
              "enableRemoteModule": true
            }
          }
        }
      }
    }
  },
